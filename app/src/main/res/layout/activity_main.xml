        android:textColor="#3F00FF"
        android:textSize="27sp"
        android:id="@+id/result_mobilenet"
        android:layout_below="@id/effnet_time"
        android:layout_marginTop="15sp"
        android:visibility="gone"
        />
    <TextView
        android:layout_width="wrap_content"
